import request from "@/utils/request";

// 条件查询运单列表
export function getList(data){
    return request({
        url: "/operation/carriageAudit/queryCarriages",
        method: "post",
        data: data
    })
}

//审核
export function carriageAudit(data){
    return request({
        url: "/operation/carriageAudit/carriageAudit",
        method: "post",
        data: data
    })
}





